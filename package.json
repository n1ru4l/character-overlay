{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@n1ru4l/graphql-live-query": "0.5.0",
    "@n1ru4l/in-memory-live-query-store": "0.5.0",
    "@n1ru4l/socket-io-graphql-server": "0.7.0",
    "@prisma/client": "2.9.0",
    "@prisma/cli": "^2.9.0",
    "@tinyhttp/app": "0.5.20",
    "gqtx": "0.7.0",
    "graphql": "15.3.0",
    "patch-package": "6.2.2",
    "sirv": "1.0.7",
    "socket.io": "3.0.0"
  },
  "devDependencies": {
    "@chakra-ui/core": "1.0.0-rc.7",
    "@chakra-ui/icons": "1.0.0-rc.7",
    "@emotion/core": "10.0.35",
    "@emotion/styled": "10.0.27",
    "@graphql-codegen/cli": "1.19.0",
    "@graphql-codegen/typescript": "1.17.11",
    "@graphql-codegen/typescript-operations": "1.17.8",
    "@graphql-codegen/typescript-urql": "2.0.2",
    "@n1ru4l/socket-io-graphql-client": "^0.7.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "framer-motion": "2.9.4",
    "polished": "4.0.3",
    "prettier": "2.1.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.0",
    "socket.io-client": "3.0.0",
    "ts-node-dev": "1.0.0",
    "typescript": "^4.0.3",
    "urql": "1.10.1",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postinstall": "patch-package",
    "print-schema": "ts-node --project ./server/tsconfig.json  ./scripts/write-graphql-schema.ts",
    "codegen": "yarn print-schema && graphql-codegen --config codegen.yml",
    "server:start:dev": "ts-node-dev --project server/tsconfig.json --no-deps server/main.ts",
    "server:build": "prisma generate && tsc --project server/tsconfig.json",
    "start:prod": "prisma migrate up --experimental --create-db && node main.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
